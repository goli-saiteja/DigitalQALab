  "main": "index.js",
  "scripts": {
    "build:tailwind": "tailwind build src/styles/tailwind.css -o src/styles/tailwind.output.css",
    "watch:tailwind": "chokidar 'src/components/**/*.js' 'src/**/*.css' 'src/**/*.scss' --ignore src/styles/tailwind.output.css -c 'npm run build:tailwind'",
    "start": "npm-run-all build:tailwind --parallel watch:tailwind start:dev",
    "start:dev": "webpack-dev-server --host 0.0.0.0 --port 9020  --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "build-all": "concurrently \"webpack --config webpack.prod.js\" \"npm run build-server\"",
    "build-server": "concurrently \"http-server -a localhost -p 1234 dist\" \"open http://localhost:1234/\""
  },
